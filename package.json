{
  "name": "effsz",
  "version": "1.2.0",
  "description": "Self-confident size containers",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "test": "vitest"
  },
  "keywords": [
    "resize",
    "split",
    "scroll",
    "limit",
    "masonry",
    "web components",
    "ui",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "./split": {
      "types": "./dist/types/split.d.ts",
      "import": "./dist/split.js",
      "require": "./dist/split.js"
    },
    "./scroll": {
      "types": "./dist/types/scroll.d.ts",
      "import": "./dist/scroll.js",
      "require": "./dist/scroll.js"
    },
    "./limit": {
      "types": "./dist/types/limit.d.ts",
      "import": "./dist/limit.js",
      "require": "./dist/limit.js"
    },
    "./masonry": {
      "types": "./dist/types/masonry.d.ts",
      "import": "./dist/masonry.js",
      "require": "./dist/masonry.js"
    }
  },
  "author": "Marat Sabitov",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vitest/browser": "^2.1.9",
    "playwright": "^1.50.1",
    "rollup": "^4.30.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.9"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitverse.ru/msabitov/effsz"
  },
  "homepage": "https://effnd.tech/sz/"
}
